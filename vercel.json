{
    "version": 2,
    "name": "ambiator",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/Ambiator/(.*)", "dest": "/$1" },
      { "src": "/Ambiator", "dest": "/" }
    ]
  }
  